#Include .\WS_XML_WRITER_OUTPUT.ahk

/**
 * Specifies that the generated bytes should be placed in a buffer.
 * @see https://learn.microsoft.com/windows/win32/api/webservices/ns-webservices-ws_xml_writer_buffer_output
 * @namespace Windows.Win32.Networking.WindowsWebServices
 * @version v4.0.30319
 */
class WS_XML_WRITER_BUFFER_OUTPUT extends Win32Struct
{
    static sizeof => 4

    static packingSize => 4

    /**
     * The base type for all types that derive from <a href="https://docs.microsoft.com/windows/desktop/api/webservices/ns-webservices-ws_xml_writer_output">WS_XML_WRITER_OUTPUT</a>.
     * @type {WS_XML_WRITER_OUTPUT}
     */
    output{
        get {
            if(!this.HasProp("__output"))
                this.__output := WS_XML_WRITER_OUTPUT(0, this)
            return this.__output
        }
    }
}
