        A_LastError := 0

        result := DllCall("KERNEL32.dll\SearchPathW", "ptr", lpPath, "ptr", lpFileName, "ptr", lpExtension, "uint", nBufferLength, "ptr", lpBuffer, lpFilePartMarshal, lpFilePart, "uint")
        if(A_LastError)
